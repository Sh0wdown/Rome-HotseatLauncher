<Window x:Class="HotseatLauncher.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HotseatLauncher"
        mc:Ignorable="d"
        Height="168.601" Width="284.96">
    <Grid>
        <TextBlock x:Name="textBlock" Margin="10,9,10,91"/>
        <TextBox x:Name="textBox" Margin="41,52,37,58" />
        <Button Content="Accept" Margin="160,85,37,17" Click="Accept_Click" />
        <Button Content="Cancel" Margin="41,85,153,17" Click="Cancel_Click" />
    </Grid>
</Window>
